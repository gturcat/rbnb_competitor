
<div class="flex-container-form">
<h1>new flat</h1>

<%= simple_form_for @flat do |f| %>
  <%= f.input :address , id:"flat_address"%>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :capacity %>
  <%= f.input :price %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden%>
  <%= f.button :submit %>
<% end %>
</div>
