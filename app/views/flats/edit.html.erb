<div class="container-fluid flex-container">
  <div class="card-list"><h1>Edit my flat</h1>
    <div class="flex-container-form">
      <h1>new flat</h1>
      <%= simple_form_for @flat do |f| %>
      <%= f.input :address %>
      <%= f.input :title %>
      <%= f.input :description %>
      <%= f.input :capacity %>
      <%= f.input :price %>
      <%= cl_image_tag(@flat.photo, width: 100) %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden%>
      <%= f.button :submit %>
      <% end %>
    </div>
  </div>
</div>
